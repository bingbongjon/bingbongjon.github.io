  "devDependencies": {
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.3.12",
    "@storybook/manager-webpack5": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@svgr/webpack": "^5.5.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.1",
    "mdx-loader": "^3.0.2",
    "next-global-css": "^1.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "react-snap": "^1.23.0",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^14.0.0",
    "stylelint-config-recommended": "^6.0.0"
  },
  "dependencies": {
    "@mdx-js/react": "^1.6.22",
    "@react-spring/core": "^9.3.0",
    "@react-three/drei": "^7.19.3",
    "@react-three/fiber": "^7.0.16",
    "classnames": "^2.3.1",
    "next": "^12.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.0.2",
    "react-transition-group": "^4.4.2",
    "three": "^0.134.0"
  },
  "scripts": {
    "start": "next dev",
    "build": "next build && next export -o build/",
    "postbuild": "react-snap",
    "build-storybook": "build-storybook -o build-storybook",
    "lint": "next lint",
    "storybook": "start-storybook -p 9009 -s public --no-manager-cache",
    "analyze": "source-map-explorer 'build/_next/static/chunks/*.js'"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "skipThirdPartyRequests": true,
    "headless": true,
    "crawl": true
  },
  "browserslist": {
    "production": [
      ">10%",
      "not dead",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
